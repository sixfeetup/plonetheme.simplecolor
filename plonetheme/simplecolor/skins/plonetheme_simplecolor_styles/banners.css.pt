<tal:root define="navroot context/@@plone_portal_state/navigation_root_url">
    
#courseimagetile{
    background-image:url(<tal:path replace="navroot"/>/courseimagetile.jpg);
	background-repeat:x-repeat;
    clear: both;
    height: 120px;
    margin-left:-2%;
    margin-right:-2%;
    margin-top: 0;
    overflow: hidden;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    border-radius: 10px;
    position: relative;
}

#courseimageleft {
	background: url(<tal:path replace="navroot"/>/courseimageleft.jpg) no-repeat left top;
	position: absolute;
    height: 120px;
    width: 100%;
	}

#courseimageright {
	background: url(<tal:path replace="navroot"/>/courseimageright.gif) no-repeat right top;
	position: absolute;
    height: 120px;
    width: 100%;
	}

</tal:root>